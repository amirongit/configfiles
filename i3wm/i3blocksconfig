[xtitle]
command=scripts/xtitle/xtitle
interval=1


[iface]
command=scripts/iface/iface
IFACE=wlo1
color=#00FF00
interval=10
color=#FFFFFF
LABEL=IP

[bandwidth]
command=scripts/bandwidth/bandwidth
interval=5

[essid]
command=scripts/essid/essid
interval=60
INTERFACE=wlo1

[volume]
command=scripts/volume/volume
interval=2
signal=10
STEP=5%
LABEL=VOL

[battery]
command=scripts/battery/battery
interval=30
LABEL=BAT
color=#FFFFFF

[disk]
command=scripts/disk/disk
LABEL=DISK
interval=30

[memory]
command=scripts/memory/memory
label=MEM
interval=30

[cpu]
command=scripts/cpu_usage/cpu_usage
LABEL=CPU
interval=10

[time]
command=scripts/time/time
interval=30
STRFTIME_FORMAT=TIME %Y-%m-%d %H:%M

{
  "coc.preferences.jumpCommand": "tab drop",
  "diagnostic.messageTarget": "echo",
  "diagnostic.showUnused": true,
  "diagnostic.warningSign": ">",
  "diagnostic.errorSign": "*",
  "diagnostic.infoSign": ">",
  "diagnostic.hintSign": ">",
  "git.changeRemovedSign.text": "-",
  "git.conflictedDecorator": "*",
  "git.topRemovedSign.text": "-",
  "git.untrackedDecorator": "?",
  "git.changedDecorator": "~",
  "git.changedSign.text": "~",
  "git.removedSign.text": "-",
  "git.branchCharacter": "b",
  "git.stagedDecorator": "+",
  "git.addedSign.text": "+",
  "hover.target": "preview",
  "hover.autoHide": true,
  "pyright.organizeimports.provider": "isort",
  "python.analysis.autoImportCompletions": false,
  "python.formatting.autopep8Path": "autopep8",
  "python.analysis.typeCheckingMode": "off",
  "python.formatting.provider": "autopep8",
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Args": ["--max-line-length=120"],
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "mypy",
  "signature.hideOnTextChange": false,
  "signature.target": "echo",
  "inlayHint.enable": false
}

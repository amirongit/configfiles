[essid]
command=scripts/essid/essid
interval=60
INTERFACE=wlo1

[iface]
command=scripts/iface/iface
IFACE=wlo1
color=#00FF00
interval=10
color=#FFFFFF

[volume]
command=scripts/volume/volume
LABEL=VOL
interval=2
signal=10
STEP=5%
LABEL=VOL 

[battery]
command=scripts/battery/battery
interval=30
LABEL=BAT 

[memory]
command=scripts/memory/memory
label=MEM 
interval=30

[disk]
command=scripts/disk/disk
LABEL=DISK 
#DIR=$HOME
ALERT_LOW=10
interval=30

[time]
command=scripts/time/time
interval=30
STRFTIME_FORMAT=%Y-%m-%d %H:%M
TZ_LABELS=TEH
